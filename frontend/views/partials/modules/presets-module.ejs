<div id="left_area" class="col-xs-6">
    <!-- Area in which the presets will be generated -->
    <div class="tagsearch-container">
        <div id='tagsearch'>
            <i class="glyphicon glyphicon-search"></i>
            <input id="tagsearch_input" oninput='tagSearchInput($(this).val())' type="text"
                   class="form-control typeahead" placeholder="Search a tag..." aria-describedby="sizing-addon2">
        </div>
        <div class="btn-group btn-group-sm">
            <input id="editPresets" type="checkbox" class="btn btn-danger" onclick="loadEditablePresets()"><span class="glyphicon glyphicon-pencil"></span> Edit Presets</input>
        </div>
    </div>
    <div id="preset_area">
    </div>
</div>